// Funkcja jako argument
fun :: [int x, operator :: [int x] -> [int y] ] -> [int z]
{
  returns.z = operator(x) ;
}
dodaj :: [int x] -> [int y]
{
  returns.y = x + 7;
}

minus :: [int x] -> [int y]
{
  returns.y = x - 7;
}

// dodawanie = 14
println(fun(7, dodaj));

// odejmowanie = 0
println(fun(7, minus));
